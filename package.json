{
  "name": "alignment-ui",
  "version": "0.0.1",
  "main": "library.js",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "fasta",
    "msa",
    "bioinformatics"
  ],
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-convert-jsx-extension-to-js": "^0.0.1",
    "bootstrap": "^5.3.2",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bootswatch": "^5.3.2",
    "d3": "^7.8.5",
    "file-saver": "^2.0.5",
    "jquery": "^3.7.1",
    "phylotree": "^1.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0"
  },
  "scripts": {
    "develop": "npx webpack serve --open",
    "production": "NODE_ENV=production npx webpack",
    "library": "BABEL_ENV=library npx babel src --out-dir lib --copy-files",
    "yalc": "yarn library && yarn copy && cd lib && npx yalc publish && npx yalc push",
    "testlibrary": "yarn yalc && cd library-consumer && npx yalc add react-sass-library-moonshot && yarn develop",
    "copy": "cp {package.json,yarn.lock,README.md,web-consumer/*.js*,web-consumer/*.css} lib",
    "testdemo": "yarn production && http-server demo -o",
    "sass": "npx sass src/moonshot.scss > web-consumer/moonshot.css",
    "weblib": "NODE_ENV=production npx webpack --config webpack-weblib.config.cjs",
    "testweblib": "yarn weblib && yarn sass && http-server -o web-consumer"
  }
}
